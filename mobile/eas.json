{
  "cli": {
    "version": ">= 3.13.2",
    "promptToConfigurePushNotifications": false
  },
  "build": {
    "development": {
      "android": {
        "config": "android-build.yml"
      },
      "ios": {
        "config": "ios-build.yml"
      },
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "APP_VARIANT": "development"
      }
    },
    "preview": {
      "android": {
        "config": "android-build.yml"
      },
      "ios": {
        "config": "ios-build.yml"
      },
      "distribution": "internal",
      "env": {
        "APP_VARIANT": "preview"
      }
    },
    "production": {
      "android": {
        "config": "android-build.yml"
      },
      "ios": {
        "config": "ios-build.yml"
      },
      "env": {
        "APP_VARIANT": "production"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
